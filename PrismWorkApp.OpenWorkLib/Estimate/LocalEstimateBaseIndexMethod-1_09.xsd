            <xs:element name="Total" type="TPriceElement" minOccurs="0">
                <xs:annotation>
                    <xs:documentation>	<text>Итоги по разделу или по локальному сметному расчету</text>
	<formula>
	Для базисного уровня:
	Итого по разделу (в базисном уровне цен)
