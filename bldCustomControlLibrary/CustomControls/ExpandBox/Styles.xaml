<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <Style x:Key="ExpandedMarkerStyle" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="/bldCustomControlLibrary;component/Resourses/Images/expand.gif"/>
        <Setter Property="Height" Value="10"/>
        <Setter Property="Width"  Value="10"/>
    </Style>
    <Style x:Key="CollapsedMarkerStyle" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="/bldCustomControlLibrary;component/Resourses/Images/collapse.gif"/>
        <Setter Property="Height" Value="10"/>
        <Setter Property="Width"  Value="10"/>
    </Style>
    
</ResourceDictionary>