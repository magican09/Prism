<RibbonGroup x:Class="PrismWorkApp.Modules.BuildingModule.Views.DBIteractionRibbonGroupView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrismWorkApp.Modules.BuildingModule.Views"
             mc:Ignorable="d" Header="Обмен данными" >
    <StackPanel Orientation="Horizontal">
        <RibbonButton Label="Импорт серт. пасп. из Access"   LargeImageSource="/PrismWorkApp.Modules.BuildingModule;component/Resourses/Images/Ribbon/access_icon.png"
                      Command="{Binding LoadMaterialCertificatesFromAccessCommand}"/>
        <RibbonButton Label ="Загрузить перечень из БД" LargeImageSource="/PrismWorkApp.Modules.BuildingModule;component/Resourses/Images/Ribbon/PNG/128x128/load_AggregationDocument.png" 
                Command="{Binding ApplicationCommands.LoadAggregationDocumentsFromDBCommand}"/>
    </StackPanel>
</RibbonGroup>
