<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:bldCustomControlLibrary">

    <Style TargetType="{x:Type local:DiargamDataGridColumnHeadersPresenter}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type local:DiargamDataGridColumnHeadersPresenter}">
                    <Grid>
                        <local:DiagramDataGridColumnHeader x:Name="PART_FillerColumnHeader"
                                              IsHitTestVisible="False"/>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        
    </Style>
   
</ResourceDictionary>