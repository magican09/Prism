<UserControl x:Class="PrismWorkApp.ResourceLibrary.Themes.TestControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PrismWorkApp.ResourceLibrary.Themes"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <UserControl.Resources>
        <Style x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type local:TestClass},
        ResourceId=TessStyle}" TargetType="TextBlock">
            <Setter Property="Background" Value="Aqua"/>
        </Style>
    </UserControl.Resources>
    <Grid>
            
    </Grid>
</UserControl>
